$ let _ = require('underscore');
$ let fieldId = data.name.replace(['[', ']'], '_');
$ let fields = _.isObject(data.fields) ? data.fields : {};

<div class="input-field nb-date-time-field col s12 m${fields.cols}">
    <input id="${fieldId}" type="text" value="${fields.value}" class="datepicker form-control" placeholder="${fields.helper}" />
    <label for="${fieldId}" class="">${fields.label}</label>
</div>