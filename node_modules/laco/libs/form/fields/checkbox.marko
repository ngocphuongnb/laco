static {
    const path = require('path');
    const _ = require('underscore');
    const helper = lacoLib('./libs/helper');
}

$ let fields = _.isObject(data.fields) ? data.fields : {};
$ let options = _.isObject(fields.options) ? fields.options : {};
$ let fieldId = data.name.replace(['[', ']'], '_');

<div id="${fieldId}" class="nb-checkbox-group col s12 m${fields.cols}">
    <div class="input-field nb-checkbox-field-label"><b>${fields.label}</b></div>
    <div class="input-field nb-checkbox-field" for(ov, ok in fields.options)>
        $ let checked = (fields.value.indexOf(ok) > -1) ? 'checked' : '';
        <input name="${fields.name}[]" class="filled-in" id="route-selected-${ok}" type="checkbox" value="${ok}" ${checked} />
        <label for="route-selected-${ok}">${ov}</label>
    </div>';
</div>