$ let common = data._common;
<lasso-page dependencies=common.dependencies flags=common.pageFlags />
<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <title>Laco admin</title>
    <lasso-head />
</head>

<body class="${common.pageFlags.join(' ')} theme-cyan">
    <section class="content" style="margin: 0">
        <div class="container-fluid" style="padding: 15px">
            <ol class="breadcrumb" id="laco-admin-breadcrumb">
                $ let breadcrumbsCount = common.breadcrumbs.length - 1;
                <li for(index, br in common.breadcrumbs) class=((breadcrumbsCount == index) && 'active')>
                    <a href="${br.path}">${br.name}</a>
                </li>
            </ol>
            <div class="row clearfix">
                <div id="laco-main-content" class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <include(input.body)/>
                </div>
            </div>
        </div>
    </section>
    <lasso-body />
</body>
</html>